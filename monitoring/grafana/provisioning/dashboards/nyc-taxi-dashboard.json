{
  "id": null,
  "title": "NYC Taxi ML Model Monitoring",
  "tags": ["mlops", "taxi", "predictions"],
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "title": "Total API Requests",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(api_requests_total)",
          "legendFormat": "Total Requests",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 100},
              {"color": "red", "value": 1000}
            ]
          },
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
    },
    {
      "id": 2,
      "title": "Request Rate (per minute)",
      "type": "stat", 
      "targets": [
        {
          "expr": "rate(api_requests_total[1m]) * 60",
          "legendFormat": "Requests/min",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "thresholds"},
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 10},
              {"color": "red", "value": 50}
            ]
          },
          "unit": "reqps"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
    },
    {
      "id": 3,
      "title": "Total Model Predictions",
      "type": "stat",
      "targets": [
        {
          "expr": "sum(model_predictions_total)",
          "legendFormat": "Total Predictions",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "short"
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 12, "y": 0}
    },
    {
      "id": 4,
      "title": "Active Predictions",
      "type": "gauge",
      "targets": [
        {
          "expr": "active_predictions",
          "legendFormat": "Active",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "max": 10,
          "min": 0,
          "thresholds": {
            "steps": [
              {"color": "green", "value": null},
              {"color": "yellow", "value": 5},
              {"color": "red", "value": 8}
            ]
          }
        }
      },
      "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
    },
    {
      "id": 5,
      "title": "API Response Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(api_request_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(api_request_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
    },
    {
      "id": 6,
      "title": "Model Prediction Time",
      "type": "timeseries",
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(prediction_duration_seconds_bucket[5m]))",
          "legendFormat": "50th percentile",
          "refId": "A"
        },
        {
          "expr": "histogram_quantile(0.95, rate(prediction_duration_seconds_bucket[5m]))",
          "legendFormat": "95th percentile",
          "refId": "B"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": {"mode": "palette-classic"},
          "unit": "s"
        }
      },
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
    }
  ],
  "time": {"from": "now-1h", "to": "now"},
  "refresh": "30s",
  "schemaVersion": 36,
  "version": 1
}
